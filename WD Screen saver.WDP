     code : |1-
      
      WHEN EXCEPTION
      	EndProgram()
      END
      
      EXTERN "winconst.wl"
      
      gbScreenSaverMode  is boolean
      gnWidthAllScreens  is system int
      gnHeightAllScreens  is system int
      gnXPosAllScreens     is system int
      gnYPosAllScreens     is system int
      gbPreviewMode is boolean
      
      //
      // Initializations
      //
      GetScreenInformation()
      
      // If the extension of the file is '.scr', the mode is Screen Saver
      gbScreenSaverMode = fExtractPath(ExeInfo(exeName), fExtension) ~= ".scr"
      
      // Process the command line
      ProcessCommandLine()
