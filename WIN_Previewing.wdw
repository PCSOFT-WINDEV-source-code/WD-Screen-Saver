     code : |1-
      
      gnHwnd is system int
      
      // Retrieves the handle passed in parameter
      gnHwnd = Val(NoSpace(ExtractString(Upper(CommandLine()), 2, " ")))
      
      cvRect is composed of 
      	nLeft   is int
      	nTop    is int
      	nRight  is int
      	nBottom is int
      END
      
      // Retrieves the dimensions of the preview window of Windows
      API("user32", "GetClientRect", gnHwnd, &cvRect)
      
      // Resizes the window
      MyWindow..Width = cvRect.nRight
      MyWindow..Height = cvRect.nBottom	
      
      // Indicates that our window becomes a child window of the preview window of Windows
      API("user32", "SetParent", Handle(), gnHwnd)
      
      // The preview window of Windows takes focus
      SysSetFocus(gnHwnd)
